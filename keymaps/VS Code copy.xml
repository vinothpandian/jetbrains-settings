<keymap version="1" name="VS Code copy">
  <action id="ActivateTerminalToolWindow">
    <keyboard-shortcut first-keystroke="ctrl #10000d6" />
  </action>
  <action id="CommentByBlockComment">
    <keyboard-shortcut first-keystroke="shift alt a" />
    <keyboard-shortcut first-keystroke="shift ctrl number_sign" />
  </action>
  <action id="CommentByLineComment">
    <keyboard-shortcut first-keystroke="ctrl k" second-keystroke="ctrl c" />
    <keyboard-shortcut first-keystroke="ctrl number_sign" />
  </action>
</keymap>